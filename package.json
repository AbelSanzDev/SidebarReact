{
  "name": "sidebar-react-pro",
  "version": "1.0.9",
  "description": "Welcome to our modern and responsive sidebar solution! Designed to adapt seamlessly across all devices and screen sizes, our sidebar enhances user experience by providing easy navigation and accessibility in various contexts.",
  "main": "dist/index.umd.js",
  "module": "dist/index.es.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    }
  },
  "files": [
    "/dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "prepare": "npm run build",
    "preview": "vite preview"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AbelSanzDev/SidebarReact.git"
  },
  "keywords": [
    "react",
    "vite",
    "eslint",
    "tailwindcss"
  ],
  "author": "Abel Sanchez Urrea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AbelSanzDev/SidebarReact/issues"
  },
  "homepage": "https://github.com/AbelSanzDev/SidebarReact#readme",
  "private": false,
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.55.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "vite": "^5.0.8",
    "vite-plugin-dts": "^3.9.1"
  }
}
